Bootstrap: docker
From: ubuntu:22.04

%files
req.out /tmp/req.out

%post
	apt-get -y update
	apt-get -y install python3 python3-pip
	pip3 install \
		numpy \
		geopandas \
		rdflib \
		pandas \
		shapely \
		fiona \
		requests \
		colorama \
		ontospy \
		matplotlib \
		tqdm \
		pyproj \
		s2sphere

pip install -r /tmp/req.out
